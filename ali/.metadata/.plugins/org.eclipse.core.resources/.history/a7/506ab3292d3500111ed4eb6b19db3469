class formulario_Principal(QtGui.QMainWindow):
    def __init__(self,parent=None):
        QtGui.QWidget.__init__(self,parent)
        self.ui=Ui_Form()
        self.ui.setupUi(self)
    
    def saluda(self):
        nombre=self.ui.lineaEdit.text()
        self.ui.campoEdit.setText("Hola" + nombre)
        
